    <%= f.text_field :UserID %>
  </div>
  <div class="field">
    <%= f.label :teamNumber %><br>
    <%= f.number_field :teamNumber %>
  </div>
  <div class="field">
    <%= f.label :seedNumber %><br>
    <%= f.number_field :seedNumber %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
